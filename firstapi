const express = require('express');
const app = express ();

app.get("/post", (req,res) =>{
    res.send({
    name: "talha"
    })
})

app.get("/nodejs", (req,res)=>{
    res.send({
    name:"node.js"
    })
})
    
app.listen(3003, function(){
    console.log("Server is running on port 3003");
})
